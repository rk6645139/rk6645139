#include <stdio.h>


int main() {

    printf("program to find the sum of two numbers\n");

    int a,b,sum;

    printf(" enter the value of a");

    scanf("%d", &a);

     printf(" enter the value of b");

    scanf("%d", &b);

    sum=a+b;

    printf("sum of the given two numbers is :%d", sum);

    return 0;

}
